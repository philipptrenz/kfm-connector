{
	"name": "philipptrenz/kfm-connector",
	"description": "Keep track of all your Kirby websites within a single tool.",
	"license": "MIT",
	"type": "kirby-plugin",
	"version": "1.2.3",
	"homepage": "https://kirbyfleetmanager.com/",
	"authors": [
		{
			"name": "Philipp Trenz",
			"email": "mail@philipptrenz.de"
		}
	],
	"require": {
		"getkirby/composer-installer": "^1.1",
		"firebase/php-jwt": "^6.8"
	},
	"autoload": {
        "psr-4": {
            "PhilippTrenz\\KFMConnector\\": "src/"
        }
    },
	"config": {
		"optimize-autoloader": true,
		"allow-plugins": {
			"getkirby/composer-installer": true
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^10.3",
		"swaggest/json-schema": "^0.12.41"
	}
}
